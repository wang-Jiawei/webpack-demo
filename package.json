{
  "name": "webpack",
  "sideEffects": [
    "*.css"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "version": "1.0.0",
  "description": "",
